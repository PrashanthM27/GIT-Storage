<aura:component extends="forceCommunity:navigationMenuBase">
    <div class="slds-grid slds-grid--vertical slds-navigation-list--vertical">
    <ul onclick="{!c.onclick}">
    <aura:iteration items="{!v.menuItems}" var="v">
        <li class="{!item.active ? 'slds-is-active' :''}">
        <a href="javascript:void(0);" data-menu-item="{!item.Id}" class="slds-navigation-
         list--vertical__action slds-text-link--reset">
            {!v.label}
            </a>                                                                
        </li>
       </aura:iteration> 
        </ul>
        </div>
</aura:component>